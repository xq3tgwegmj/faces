getgenv().faces = {[1]="Right, face.", [2]="Left, face.", [3]="About, face.", [4]="Right, incline.", [5]="Left, incline.", [6]="About, face.", [7]="Jump face.", [8]="Control, face.", [9]="Center, face."}

-- Gui to Lua
-- Version: 3.2

-- Instances:

